<div class="row">
  <div class="cell" data-title="Date">
    {{this.created_at}}
  </div>
  <div class="cell" data-title="obj">
    {{this.objectives}}
  </div>
  <div class="cell" data-title="rev">
    {{this.total_revenue}}
  </div>
  <div class="cell" data-title="area">
    {{this.areas}}
  </div>
  <div class="cell" data-title="works">
    {{this.construction_works}}
  </div>
  <div class="cell" data-title="cart">
    <a href="/{{this._id}}" class="fas fa-cart-arrow-down"></a>
  </div>
</div>

